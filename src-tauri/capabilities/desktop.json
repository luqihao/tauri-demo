{
  "identifier": "desktop-capability",
  "platforms": [
    "macOS",
    "windows",
    "linux"
  ],
  "windows": [
    "main"
  ],
  "permissions": [
    "window-state:default",
    "autostart:default",
    "global-shortcut:default",
    "fs:default",
    "fs:allow-write-text-file",
    "fs:allow-mkdir",
    "fs:allow-applog-write",
    "fs:allow-applog-write-recursive"
  ]
}